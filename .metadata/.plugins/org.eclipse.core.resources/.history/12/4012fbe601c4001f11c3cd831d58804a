package Main;

public class Project {
	private String name;
	private int duration;
	private Person person;
	
	
	Project(){
		this.name = "";
		this.duration = 0;
		this.person = "";
	}
	
	Project(String name, int duration, Person Person){
		this.name = name;
		this.duration = duration;
		this.Person = Person;
	}
}
