<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="center" spacing="10" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.ListUserControl">
   <Label alignment="CENTER" prefHeight="85.0" prefWidth="131.0" text="Table of User" />

    <TableView fx:id="userTable" prefHeight="459.0" prefWidth="732.0">
        <columns>
            <TableColumn fx:id="nameColumn" prefWidth="151.0" text="username" />
            <TableColumn fx:id="emailColumn" minWidth="0.0" prefWidth="214.0" text="Email" />
        </columns>
    </TableView>
   <HBox prefHeight="28.0" prefWidth="732.0">
      <children>
         <HBox prefHeight="28.0" prefWidth="351.0">
            <children>
               <Label text="User Name Email" fx:id="userlabel" />
            </children>
         </HBox>
      
                            <Button onAction="#handleReturn" text="Return" />
      </children>
   </HBox>
</VBox>
